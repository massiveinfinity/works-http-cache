<?xml version="1.0" encoding="utf-8"?>

<manifest
    package="com.mobilesolutionworks.android.httpcache.test"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET" />

    <application android:label="Works Http Cache Test">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".content.WorksHttpCacheServiceImpl"
            android:exported="false">
            <intent-filter>
                <action android:name="com.mobilesolutionworks.httpcache.action.GET_DATA" />
                <data
                    android:host="com.mobilesolutionworks.httpcache"
                    android:scheme="content" />
            </intent-filter>
        </service>

        <provider
            android:name="com.mobilesolutionworks.android.httpcache.WorksHttpCacheContentProvider"
            android:authorities="com.mobilesolutionworks.httpcache"
            android:exported="false" />

        <meta-data
            android:name="getDataIntent"
            android:value="com.mobilesolutionworks.httpcache.action.GET_DATA" />
    </application>
</manifest>
